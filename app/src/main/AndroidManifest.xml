<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="inm5001.rapidoservices">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission-sdk-23 android:name="android.permission.INTERNET"></uses-permission-sdk-23>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".interfaceGraphique.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".interfaceGraphique.InscriptionActivity"></activity>
        <activity android:name=".interfaceGraphique.ProfilActivity"></activity>
        <activity android:name=".interfaceGraphique.AjouterServiceActivity"></activity>
        <activity
            android:name=".interfaceGraphique.RechercheActivity"
            android:label="@string/title_activity_recherche"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>